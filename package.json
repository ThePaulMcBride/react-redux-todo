{
  "name": "react-todo",
  "version": "1.0.0",
  "description": "A simple todo app powered by Node, React and Redux",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ./test/test.js 'test/**/*.@(js|jsx)'",
    "test:watch": "npm run test --watch --watch-extensions jsx",
    "build": "webpack -d --watch --debug",
    "start": "webpack-dev-server"
  },
  "author": "Paul McBride",
  "license": "ISC",
  "dependencies": {
    "classnames": "^2.2.5",
    "immutable": "^3.8.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-immutable": "^3.0.8",
    "todomvc-app-css": "^2.0.6"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "css-loader": "^0.23.1",
    "jsdom": "^9.4.2",
    "mocha": "^3.0.2",
    "react-addons-test-utils": "^15.3.1",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.14.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  }
}
